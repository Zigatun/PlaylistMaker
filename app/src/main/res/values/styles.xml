<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MainMenuItemCard" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">22sp</item>
        <item name="android:backgroundTint">@color/yp_white</item>
        <item name="android:textColor">@color/yp_black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">16dp</item>
        <item name="iconTint">@color/yp_black</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="cornerRadius">16dp</item>
    </style>
    <style name="RegularAppButton" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">14sp</item>
        <item name="android:backgroundTint">?attr/colorButtonNormal</item>
        <item name="android:textColor">?attr/colorOnBackground</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="cornerRadius">54dp</item>
    </style>
    <style name="PageHeader">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>
    <style name="ToolbarTitleTextStyle">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>
    <style name="RegularTextStyle">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="PlaceholderErrorTextStyle">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginHorizontal">24dp</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="HistoryTitleTextStyle">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginHorizontal">16dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:layout_marginTop">24dp</item>
        <item name="android:paddingVertical">12dp</item>
    </style>
    <style name="MusicAlbumImageCard">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
    </style>
    <style name="MusicNameTextStyle">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="MusicArtistTextStyle">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="MusicNameTextStyle.Player">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="TrackInfoParamStyle">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">?attr/subtitleTextColor</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="TrackInfoValueStyle">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:layout_width">wrap_content</item>
    </style>
    <style name="TrackCursorTextStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">30dp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="MusicArtistTextStyle.Player">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="SettingsElementHeader">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>
    <style name="SettingsElementSwitch">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">?attr/colorOnPrimary</item>
        <item name="thumbTint">@color/switch_thumb_colors</item>
        <item name="trackTint">@color/switch_track_colors</item>
    </style>
    <style name="SettingsElementIcon">
        <item name="android:layout_gravity">end</item>
        <item name="android:layout_marginVertical">16dp</item>
        <item name="android:backgroundTint">@color/yp_text_gray</item>
    </style>
</resources>